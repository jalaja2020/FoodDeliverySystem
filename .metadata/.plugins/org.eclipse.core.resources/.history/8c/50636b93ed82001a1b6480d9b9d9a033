package com.example.entity;

import javax.persistence.Entity;
import javax.persistence.GeneratedValue;
import javax.persistence.Id;
import javax.persistence.SequenceGenerator;
import javax.persistence.Table;

@Entity
@Table(name="items")

public class Items {
	@Id
	@SequenceGenerator(name = "mySeqItemsGen", sequenceName = "mySeq", initialValue = 100, allocationSize = 200)
	@GeneratedValue(generator = "mySeqItemsGen")
	Integer id;
	String itemName;
	String itemDesc;
	float price;
}
